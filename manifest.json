{
    "name": "LinkedOut",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup/linked-out.html",
      "default_icon": "images/logo_128x128.png"
    },

    "background": {

        "service_worker": "background.js"
    },

    "permissions": ["storage"],

    "content_scripts": [
    {
      "css": ["content.css"],
      "js": ["content.js"],
      "matches": ["https://mail.google.com/*"]
    }
  ],

  "web_accessible_resources": [
    {
      "resources": ["images/*.png"],
      "matches": ["<all_urls>"]
    }
  ]
}